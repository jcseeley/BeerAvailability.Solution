@using BeerAvailability.Models;
@model List<Brewery>;



@if (@Model.Count == 0)
{
  
} 
else
{
  <h1>Brewery Details:</h1>
  @foreach (Brewery brewery in Model)
  {
    <ul>
        <li>Name: @brewery.Name</li>
        <li>Street: @brewery.Street</li>
        <li>City: @brewery.City</li>
        <li>State: @brewery.State</li>
        <li>Website: @brewery.WebsiteURL</li>
    </ul>
    <br>
  }
}